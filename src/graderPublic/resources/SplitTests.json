  "testSplitRoot": [
    {
      "tree": ["A", "B", "C", "D", "E"],
      "degree": 3,
      "index": 0,
      "expectedParentIndex": 0,
      "expectedChildIndex": 0,
      "expected": [["A", "B"], "C", ["D", "E"]]
    },
    {
      "tree": ["A", "B", "C", "D", "E"],
      "degree": 3,
      "index": 1,
      "expectedParentIndex": 0,
      "expectedChildIndex": 1,
      "expected": [["A", "B"], "C", ["D", "E"]]
    },
    {
      "tree": ["A", "B", "C", "D", "E"],
      "degree": 3,
      "index": 2,
      "expectedParentIndex": 0,
      "expectedChildIndex": 2,
      "expected": [["A", "B"], "C", ["D", "E"]]
    },
    {
      "tree": ["A", "B", "C", "D", "E"],
      "degree": 3,
      "index": 3,
      "expectedParentIndex": 1,
      "expectedChildIndex": 0,
      "expected": [["A", "B"], "C", ["D", "E"]]
    },
    {
      "tree": ["A", "B", "C", "D", "E"],
      "degree": 3,
      "index": 4,
      "expectedParentIndex": 1,
      "expectedChildIndex": 1,
      "expected": [["A", "B"], "C", ["D", "E"]]
    },
    {
      "tree": ["A", "B", "C", "D", "E"],
      "degree": 3,
      "index": 5,
      "expectedParentIndex": 1,
      "expectedChildIndex": 2,
      "expected": [["A", "B"], "C", ["D", "E"]]
